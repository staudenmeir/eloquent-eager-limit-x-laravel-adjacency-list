{
    "name": "staudenmeir/eloquent-eager-limit-x-laravel-adjacency-list",
    "description": "Merge of staudenmeir/eloquent-eager-limit and staudenmeir/laravel-adjacency-list",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0",
        "staudenmeir/eloquent-eager-limit-x-laravel-cte": "^1.1.1",
        "staudenmeir/laravel-adjacency-list": "^1.13.5"
    },
    "require-dev": {
        "doctrine/dbal": "^3.5.2",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.1",
        "staudenmeir/eloquent-has-many-deep": "^1.18"
    },
    "autoload": {
        "psr-4": {
            "Staudenmeir\\EloquentEagerLimitXLaravelAdjacencyList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Staudenmeir\\EloquentEagerLimitXLaravelAdjacencyList\\Tests\\": "tests",
            "Staudenmeir\\EloquentEagerLimit\\Tests\\": "tests/EloquentEagerLimit",
            "Staudenmeir\\LaravelAdjacencyList\\Tests\\": "tests/LaravelAdjacencyList"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
